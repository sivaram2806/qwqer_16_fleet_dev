<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form_extended" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.qwqer.base</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="0"/>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='recaptcha']" position="after">
                <div class="col-12 col-lg-6 o_setting_box" id="qwqer_base_cashfree_env">
                    <div class="o_setting_right_pane" id="web_cashfree_env_settings">
                        <label string="Cash Free Environment" for="cashfree_env"/>
                        <div>
                            <field name="cashfree_env"/>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
